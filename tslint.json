{
  "defaultSeverity": "error",
  "extends": ["tslint:all", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "curly": [true, "as-needed"],
    "file-name-casing": [false, "camel-case"],
    "typedef": false,
    "strict-boolean-expressions": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules", "dist"]
  }
}
